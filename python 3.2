import math
number1 = int (input ('enter number one?'))
number2 = int (input ('enter number two?'))
equal = input ('+,-,*,/,radical,sin,cos,tan,cot,factorial ?')

if equal == "+" :
    print (number1 + number2)

elif equal == "-" :
    print (number1 - number2)

elif equal == "*" :
    print (number1 * number2)

elif equal == "/" :
    if number2 == 0 :
        print ("can't divide by zero")
    else :
        print (number1 / number2)

elif equal == "radical" :
    print (math.sqrt(number1))
    print (math.sqrt(number2))

elif equal == "sin" :
    print (math.sin(number1))
    print (math.sin(number2))

elif equal == "cos" :
    print (math.cos(number1))
    print (math.cos(number2))

elif equal == "tan" :
    print (math.tan(number1))
    print (math.tan(number2))

elif equal == "cot" :
    if number1 :
        print (math.cos(number1)/math.sin(number1))
    else :
        print ("can't number1 divide by zero")
    if number2 :
        print (math.cos(number2)/math.sin(number2))
    else :
        print ("can't number2 divide by zero")

elif equal == "factorial" :
    print (math.factorial(number1))
    print (math.factorial(number2))